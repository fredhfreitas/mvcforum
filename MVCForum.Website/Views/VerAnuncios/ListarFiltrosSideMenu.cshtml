@model List<MvcForum.Web.ViewModels.CategoriaAnuncio.CategoriaAnuncioViewModel>

@{
    Layout = null;
}
<div class="categoryindex">
    <div class="side-box categories-box">
        <h3 class="title-bar">Filtro</h3>
        <h5>Escolha uma ou mais opções para filtrar sua busca</h5>
        <ul>
            @foreach (var categoriaAnuncio in Model)
            {
                <li class="catitem">
                    <a style="border-color: #ced1d1" href="@categoriaAnuncio.URLTitulo">@categoriaAnuncio.NomeTitulo</a>
                </li>
            }
        </ul>
    </div>
</div>